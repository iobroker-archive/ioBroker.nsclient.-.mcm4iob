{
    "i18n": true,
    "type": "tabs",
    "tabsStyle": {
        "width": "calc(100% - 100px)"
    },	
    "items": {
        "devices": {
            "type": "panel",
            "label": "lblDevs",
            "items": {                        
                "devs": {
                    "type": "table",
		    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12,
                    "label": "lblDevsDevices",
                    "showSecondAddAt":5,
                    "items": [
                        {
                            "type": "checkbox",
                            "attr": "devAct",
                            "width": "5% ",
                            "title": "lblDevAct",
                            "filter": false,
                            "sort": false,
                            "default": true
                        },
                        {
                            "type": "text",
                            "attr": "devName",
                            "width": "20% ",
                            "title": "lblDevName",
                            "filter": false,
                            "sort": false,
                            "default": "",
                            "validator": "return(data.devName != '')",
                            "validatorErrorText" : "errDevName",
                            "validatorNoSaveOnError" : true                            
                        },
                        {
                            "type": "text",
                            "attr": "devIpAddr",
                            "width": "20% ",
                            "title": "lblDevIpAddr",
                            "filter": false,
                            "sort": false,
                            "default": "0.0.0.0"
                        },
                        {
                            "type": "text",
                            "attr": "devUser",
                            "width": "20% ",
                            "title": "lblDevUser",
                            "filter": false,
                            "sort": false,
                            "default": ""
                        },
                        {
                            "type": "password",
                            "attr": "devPwd",
                            "width": "30% ",
                            "title": "lblDevPwd",
                            "visible": true,
                            "filter": false,
                            "sort": false,
                            "default": ""
                        },
                        {
                            "type": "number",
                            "attr": "devPollIntvl",
                            "width": "5% ",
                            "title": "lblDevPollIntvl",
                            "filter": false,
                            "sort": false,
                            "min": 5,
                            "max": 3600,
                            "default": "30"
                        },
                        {
                            "type": "number",
                            "attr": "devTimeout",
                            "width": "5% ",
                            "title": "lblDevTimeout",
                            "filter": false,
                            "sort": false,
                            "min": 1,
                            "max": 600,
                            "default": "5"
                        },
                        {
                            "type": "checkbox",
                            "attr": "devChkCpu",
                            "width": "5% ",
                            "title": "lblDevChkCpu",
                            "filter": false,
                            "sort": false,
                            "default": true
                        },
                        {
                            "type": "checkbox",
                            "attr": "devChkMem",
                            "width": "5% ",
                            "title": "lblDevChkMem",
                            "filter": false,
                            "sort": false,
                            "default": true
                        },
                        {
                            "type": "checkbox",
                            "attr": "devChkDrives",
                            "width": "5% ",
                            "title": "lblDevChkDrives",
                            "filter": false,
                            "sort": false,
                            "default": true
                        }
                    ]
                }
            }
        }
    }
}
